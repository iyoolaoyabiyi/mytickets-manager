<header class="c-page-header">
  <h1 class="c-page-header__title">{{ title }}</h1>
  {% if subtitle is defined and subtitle %}
    <p class="c-page-header__subtitle">{{ subtitle }}</p>
  {% endif %}
  {% if actions_html is defined and actions_html %}
    <div class="c-page-header__actions">{{ actions_html|raw }}</div>
  {% endif %}
</header>
